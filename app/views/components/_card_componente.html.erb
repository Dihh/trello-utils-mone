<div class="card" id="<%= dom_id element %>">
  <div class="card-header">
    <h4 class="card-title"><%= title %></h4>
  </div>
  <div class="card-body">
    <% fields.each do |field| %>
      <div class="row">
        <div class="col s1"><strong><%= field %>:</strong></div>
        <div class="col s1"><%= element[field] %></div>
      </div>
    <% end %>
  </div>
</div>