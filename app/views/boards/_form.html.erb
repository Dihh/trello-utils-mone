<% form_content = form_with(model: board) do |form| %>
  <%= render "components/form-components/input_text",  form: form, field: "trello_id"%>
  <%= render "components/form-components/input_text",  form: form, field: "name"%>
  <%= render "components/form-components/input_text",  form: form, field: "user_id"%>
  <%= render "components/form-components/submit",  form: form%>
<% end %>

<%= render "components/form",  form_content: form_content, element: board%>