<%= render "components/card_componente",  element: board, title: "Board", fields: ["id", "name", "trello_id", "user_id"]  %>

<% lists_table = render "components/table",  elements: @board.lists, headers: ["name", "trello_id", "board_id"]  %>
<% labels_table = render "components/table",  elements: @board.labels, headers: ["name", "trello_id", "board_id"]  %>
<% analyses_table = render "components/table",  elements: @board.analyses, headers: ["name", "board_id", "pre_planning", "pos_planning", "start_date", "end_date", "status"]  %>
<% recurrent_cards_table = render "components/table",  elements: @board.recurrent_cards, headers: ["name", "board_id"]  %>

<%= render "components/card_table",  table: lists_table, title: "Lists"  %>
<%= render "components/card_table",  table: labels_table, title: "Labels"  %>

<%= link_to "New analysis" , new_analysis_path %>
<%= render "components/card_table",  table: analyses_table, title: "Analyses"  %>

<%= link_to "New cecurrent card" , new_recurrent_card_path %>
<%= render "components/card_table",  table: recurrent_cards_table, title: "Recurrent cards"  %>
