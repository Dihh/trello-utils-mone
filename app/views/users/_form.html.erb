<% form_content = form_with(model: user) do |form| %>
  <%= render "components/form-components/input_text",  form: form, field: "key"%>
  <%= render "components/form-components/input_text",  form: form, field: "token"%>
  <%= render "components/form-components/submit",  form: form%>
<% end %>

<%= render "components/form",  form_content: form_content, element: user%>