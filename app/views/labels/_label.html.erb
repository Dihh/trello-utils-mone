<div class="card" id="<%= dom_id label %>">
  <div class="card-header">
    <h4 class="card-title">Label</h4>
  </div>
  <div class="card-body">
    <div class="row">
        <div class="col s1"><strong>Id:</strong></div>
        <div class="col s1"><%= label.id %></div>
    </div>
    <div class="row">
        <div class="col s1"><strong>Name:</strong></div>
        <div class="col s1"><%= label.name %></div>
    </div>
    <div class="row">
        <div class="col s1"><strong>Board Id:</strong></div>
        <div class="col s1"><%= label.board_id %></div>
    </div>
  </div>
</div>